{
  "version": 2,
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.6.0"
    },
    "ai-editor/api/**/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/(.*).php",
      "dest": "/$1.php"
    },
    {
      "src": "/ai-editor/(.*)",
      "dest": "/ai-editor/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "DB_HOST": "@db_host",
    "DB_NAME": "@db_name",
    "DB_USER": "@db_user",
    "DB_PASS": "@db_pass",
    "AI_OPENAI_ENDPOINT": "@ai_openai_endpoint",
    "AI_OPENAI_KEY": "@ai_openai_key",
    "AI_SSH_ENCRYPTION_KEY": "@ai_ssh_encryption_key"
  },
  "build": {
    "env": {
      "PHP_VERSION": "8.2"
    }
  }
}
